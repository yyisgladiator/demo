package abp;

/* Unfair Medium */
<<nonDeterministic>>component Medium<E> {

  port
    in E i,
    out E o;

  automaton Medium {
    variable int coin;

    state Single;
    initial Single / {coin=alt{0,1}};

    Single [coin==0] / {coin=alt{0,1}};
    Single [coin==1] / {coin=alt{0,1}, o=i};
  }
}
